  <test name="Element identique composé">
    <contenu>
      <composé>Identique</composé>
    </contenu>
  </test>
  <test name="Element ajouté vide"/>
  <test name="Element ajouté debut">
    <element id="2"/>
    <element id="3"/>
    <element id="4"/>
  </test>
  <test name="Element ajouté milieu">
    <element id="2"/>
    <element id="3"/>
    <element id="4"/>
  </test>
  <test name="Element ajouté fin">
    <element id="2"/>
    <element id="3"/>
    <element id="4"/>
  </test>
  <test name="Element supprimé vide">
    <element id="1"/>
  </test>
  <test name="Element supprimé début">
    <element id="1"/>
    <element id="2"/>
    <element id="3"/>
    <element id="4"/>
    <element id="5"/>
  </test>
  <test name="Element supprimé milieu">
    <element id="1"/>
    <element id="2"/>
    <element id="3"/>
    <element id="4"/>
    <element id="5"/>
  </test>
  <test name="Element supprimé fin">
    <element id="1"/>
    <element id="2"/>
    <element id="3"/>
    <element id="4"/>
    <element id="5"/>
  </test>
  <test name="Element ajouté sans ordre">
    <element>1</element>
    <element>3</element>
    <element>4</element>
    <element>5</element>
  </test>
  <test name="Element supprimé sans ordre">
    <element>1</element>
    <element>2</element>
    <element>3</element>
    <element>4</element>
    <element>5</element>
  </test>
  <test name="Ajout elements variés"/>
  <test name="Suppression elements variés"><![CDATA[ Element CData ]]><?element Processing instruction ?><!-- Commentaire -->
  Texte avec &lt; &gt; caractères spéciaux &amp;
 </test>
  <test name="Elements variés identiques"><![CDATA[ Element CData ]]><?element Processing instruction ?><!-- Commentaire -->
  Texte avec &lt; &gt; caractères spéciaux &amp;
 </test>
  <test name="Elements variés modifiés"><![CDATA[ Element CData - avant ]]><?element Processing instruction - avant ?><!-- Commentaire - avant -->
  Texte avec &lt; &gt; caractères spéciaux &amp; avant
 </test>
  <test name="Ajout d'un fils unique">
    <root>
      <child/>
    </root>
  </test>
  <test name="Suppression d'un fils unique">
    <root>
      <child>
        <element/>
      </child>
    </root>
  </test>
  <test name="Modification de texte">Texte avant</test>
  <test name="Texte variant suivant retour chariots entre balises">
 Texte
 <balise/>
 Texte
 <balise>
 Balise
 </balise>
 </test>
  <test name="Attribut identique mais inversés et espacés">
    <element att2="val2" att3="val3" att1="val1"/>
  </test>
  <test name="Ajout attribut seul">
    <element/>
  </test>
  <test name="Ajout attribut debut">
    <element att2="val2" att3="val3"/>
  </test>
  <test name="Ajout attribut milieu">
    <element att1="val1" att3="val3"/>
  </test>
  <test name="Ajout attribut fin">
    <element att1="val1" att2="val2"/>
  </test>
  <test name="Suppression attribut seul">
    <element attribut="valeur"/>
  </test>
  <test name="Suppression attribut début">
    <element att1="val1" att2="val2" att3="val3"/>
  </test>
  <test name="Suppression attribut milieu">
    <element att1="val1" att2="val2" att3="val3"/>
  </test>
  <test name="Suppression attribut fin">
    <element att1="val1" att2="val2" att3="val3"/>
  </test>
  <test name="Modification attribut">
    <element att1="val1" att="val-avant" att3="val3"/>
  </test>
  <test name="Modification attribut depuis vide">
    <element att1="val1" att="" att3="val3"/>
  </test>
  <test name="Modification attribut vers vide">
    <element att1="val1" att="val" att3="val3"/>
  </test>
  <test name="Modification d'un attribut identifiant">
    <element id="avant"/>
  </test>
</tests>
